{
  "manifest_version": 2,
  "name": "TON safe",
  "description": "wallet",
  "version": "1.0",
  "permissions": [
    "storage"
],
"icons": {
  "16": "icons/16.png",
  "32": "icons/32.png",
  "128": "icons/128.png"
},
"browser_action": {
  "default_popup": "index.html"
},
"content_security_policy": "script-src 'self' 'wasm-eval' 'unsafe-eval'; object-src 'self'"
}
